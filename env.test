
# name of container holding the katas volume
export CYBER_DOJO_KATA_DATA_CONTAINER_NAME=cyber-dojo-git-kata-DATA-CONTAINER

# the path the katas volume is mapped to
export CYBER_DOJO_KATAS_ROOT=/tmp/katas

# where simplecov saves to
export CYBER_DOJO_COVERAGE_ROOT=/tmp/coverage

# the path the start_points volume is mapped to
export CYBER_DOJO_START_POINTS_ROOT=/app/start_points